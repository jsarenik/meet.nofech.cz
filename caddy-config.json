{
  "apps": {
    "http": {
      "servers": {
        "srv0": {
          "listen": [
            ":443"
          ],
          "routes": [
            {
              "handle": [
                {
                  "handler": "subroute",
                  "routes": [
                    {
                      "handle": [
                        {
                          "handler": "vars",
                          "root": "/home/nisim/web/meet.nofech.cz/public"
                        }
                      ]
                    },
                    {
                      "handle": [
                        {
                          "handler": "headers",
                          "response": {
                            "set": {
                              "Cache-Control": [
                                "max-age=31536000"
                              ]
                            }
                          }
                        }
                      ],
                      "match": [
                        {
                          "path": [
                            "/*"
                          ]
                        }
                      ]
                    },
                    {
                      "handle": [
                        {
                          "encodings": {
                            "gzip": {},
                            "zstd": {}
                          },
                          "handler": "encode"
                        }
                      ]
                    },
                    {
                      "group": "group17",
                      "handle": [
                        {
                          "handler": "subroute"
                        }
                      ],
                      "match": [
                        {
                          "path": [
                            "/sounds/*",
                            "/libs/*",
                            "/lang/*",
                            "/css/*",
                            "/images/*",
                            "/static/*"
                          ]
                        }
                      ]
                    },
                    {
                      "group": "group17",
                      "handle": [
                        {
                          "handler": "subroute",
                          "routes": [
                            {
                              "handle": [
                                {
                                  "handler": "reverse_proxy",
                                  "upstreams": [
                                    {
                                      "dial": "127.0.0.1:12345"
                                    }
                                  ]
                                }
                              ]
                            }
                          ]
                        }
                      ],
                      "match": [
                        {
                          "path": [
                            "/http-bind*"
                          ]
                        }
                      ]
                    },
                    {
                      "group": "group17",
                      "handle": [
                        {
                          "handler": "subroute",
                          "routes": [
                            {
                              "group": "group7",
                              "handle": [
                                {
                                  "handler": "rewrite",
                                  "uri": "/index.html"
                                }
                              ]
                            }
                          ]
                        }
                      ],
                      "match": [
                        {
                          "path": [
                            "/*"
                          ]
                        }
                      ]
                    },
                    {
                      "handle": [
                        {
                          "handler": "file_server",
                          "hide": [
                            "/root/caddy/Caddyfile"
                          ]
                        }
                      ]
                    }
                  ]
                }
              ],
              "match": [
                {
                  "host": [
                    "meet.nofech.cz",
                    "jitsi.nofech.cz"
                  ]
                }
              ],
              "terminal": true
            },
            {
